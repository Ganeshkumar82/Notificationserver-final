{
  "name": "svmnotificationserver",
  "version": "2.0.0",
  "main": "index.js",
  "description": "Notification server for sending the Real time Event notification among the connected Users.",
  "scripts": {
    "packager": "electron-packager . --platform=win32 --arch=x64 --out=dist --overwrite",
    "builder": "electron-builder --win",
    "start": "electron ."
  },
  "build": {
    "appId": "www.sporada.in",
    "productName": "Notification Server",
    "compression": "maximum",
    "directories": {
      "output": "build1"
    },
    "win": {
      "icon": "notification.ico"
    }
  },
  "author": "Ganesh kumar",
  "dependencies": {
    "@grpc/grpc-js": "^1.10.9",
    "@grpc/proto-loader": "^0.7.13",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "fs": "^0.0.1-security",
    "http": "^0.0.1-security",
    "ioredis": "^5.3.2",
    "mysql2": "^3.9.2",
    "net": "^1.0.2",
    "node": "^21.6.1",
    "node-cache": "^5.1.2",
    "os": "^0.1.2",
    "queue": "^7.0.0",
    "redis": "^4.6.13",
    "url": "^0.11.3",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "electron": "^33.0.2",
    "electron-builder": "^24.13.3"
  }
}
